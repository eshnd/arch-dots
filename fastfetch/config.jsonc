{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "small",
    "color": {
      "1": "1;#ffffff",
      "2": "1;#ffffff"
    },
    "padding": {
      "top": 1
    }
  },
  "display": {
    "separator": "",
    "color": {
      "keys": "1;94",
      "separator": "0;94"
    },
    "key": { "width": 10 }
  },
  "modules": [
    {
      "type": "title",
      "color": {
        "user": "1;94",
        "at": "0;#ffffff",
        "host": "1;94"
      }
    },
    "break",
    {
      "type": "os",
      "format": "{2}"
    },
    {
      "type": "wm",
      "format": "{1}"
    },
    {
      "type": "shell",
      "format": "{1}"
    },
    "uptime",
    {
      "type": "packages",
      "format": "{2} + {44}"
    },
    "break",
    {
      "type":"colors",
      "symbol":"circle"
    },
    "break"
  ]
}
